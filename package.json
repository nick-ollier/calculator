{
    "name": "calculator",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 9999",
        "build": "next build",
        "start": "next start -p 8888",
        "storybook": "start-storybook -s ./public -p 6006",
        "build-storybook": "build-storybook -s public",
        "lint": "eslint '*/**/*.{js,jsx,ts,tsx}' --quiet",
        "lint:fix": "eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
        "make-badges": "istanbul-badges-readme",
        "prettier": "prettier --check '*/**/*.{js,jsx,json,ts,tsx}'",
        "prettier:fix": "prettier --write '*/**/*.{js,jsx,json,ts,tsx}'",
        "test": "jest --silent",
        "test:watch": "jest --watch --silent",
        "test:coverage": "jest --coverage --silent",
        "cypress": "./node_modules/.bin/cypress open",
        "cypress:headless": "./node_modules/.bin/cypress run --headless --browser chrome",
        "purge": "rm -rf node_modules .next && npm i && npm run build"
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "isomorphic-dompurify": "^0.13.0",
        "mathjs": "^9.3.0",
        "next": "10.0.9",
